<Window x:Class="PictureViewer_V2.DebugWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Debug Window" MinHeight="300" Height="300" MinWidth="300" Width="400" Icon="debug_preferences.ico">
  <DockPanel LastChildFill="True">
    <Button Content="Export List" DockPanel.Dock="Top" Width="200" Margin="10,2" HorizontalAlignment="Right" Command="{Binding Path=ExportListCommand}"/>
  <ListBox ItemsSource="{Binding Path=DebugStrings}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
  </DockPanel>
</Window>
